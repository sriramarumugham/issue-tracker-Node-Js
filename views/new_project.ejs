<h1>Create Project Page</h1>

<form action="/project/create-project" method="POST" class="forms">
  <label for="name" class="labels">Name</label>
  <input id="name" name="name" required type="text"  class="inputs"/>

  <label for="description" class="labels">Description</label>
  <input id="description" name="description" required type="text" class="inputs"/>

  <label for="author" class="labels">Author</label>
  <input id="author" name="author" required type="text" class="inputs" />
  <button class="buttons">create a project</button>
</form>
